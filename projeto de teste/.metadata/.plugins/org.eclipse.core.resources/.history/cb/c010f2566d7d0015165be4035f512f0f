
public class triangulo {
	public String avalia_triangulo(int a, int b, int c) {
		boolean triangulo;
		if((a > Math.abs(b-c) && a < (b+c)) &&
			(b > Math.abs(a-c) && b < (a+c)) &&
				(c > Math.abs(a-b) && c < (b+a))){
			triangulo = true;
			
		}
		if(!triangulo){
			return "invalido"
		}
		
		return null;
	}
}
